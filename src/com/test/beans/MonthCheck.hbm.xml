<?xml version="1.0" encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.test.beans">
	<class name="MonthCheck" table="monthcheck">
		<id name="mc_id" column="mc_id">
			<generator class="native"></generator>
		</id>
		<property name="per_no" column="per_no" type="java.lang.String"
			not-null="true" length="20"></property>
		<property name="per_name" column="per_name" type="java.lang.String"
			not-null="true" length="50" />
		<property name="mc_goTime" column="mc_goTime" type="java.util.Date" />
		<property name="mc_outTime" column="mc_outTime" type="java.util.Date" />
		<property name="mc_date" column="mc_date" type="java.util.Date" />
		<property name="mc_xiu" column="mc_xiu" type="java.lang.Long"
			not-null="true" length="20" />
		<property name="mc_qing" column="mc_qing" type="java.lang.Long"
			not-null="true" length="20" />
		<property name="mc_chai" column="mc_chai" type="java.lang.Long"
			not-null="true" length="20" />
		<property name="mc_jia" column="mc_jia" type="java.lang.Long"
			not-null="true" length="20" />
		<property name="mc_chi" column="mc_chi" type="java.lang.Long"
			not-null="true" length="20" />
		<property name="mc_zao" column="mc_zao" type="java.lang.Long"
			not-null="true" length="20" />
		<property name="mc_kuang" column="mc_kuang" type="java.lang.Long"
			not-null="true" length="20" />
	</class>
</hibernate-mapping>  