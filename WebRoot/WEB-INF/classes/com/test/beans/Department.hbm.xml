<?xml version="1.0" encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.test.beans">
	<class name="Department" table="department">
		<id name="de_id" column="de_id">
			<generator class="native"></generator>
		</id>
		<property name="per_name" column="per_name" type="java.lang.String"
			not-null="true" length="50"></property>
		<property name="per_phone" column="per_phone" type="java.lang.Long"
			not-null="true" length="30" />
		<property name="de_name" column="de_name" type="java.lang.String"
			not-null="true" length="50" />
		<property name="de_info" column="de_info" type="java.lang.String"
			not-null="true" length="255" />
		<property name="de_perNum" column="de_perNum" type="java.lang.Long"
			not-null="true" length="20" />
	</class>
</hibernate-mapping>  